<Window x:Class="TestPolygons.NewCollectDB"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Создание новой коллекции в базе данных" Height="122" Width="299" ResizeMode="NoResize" Icon="Resources/Applications.ico" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" Topmost="True">
    <Grid x:Name="fmNewCollectBD" ScrollViewer.VerticalScrollBarVisibility="Disabled">
        <Button x:Name="btOk" Content="Сохранить" HorizontalAlignment="Left" Margin="10,0,0,10" Width="75" Height="22" VerticalAlignment="Bottom" Click="btOk_Click_1"/>
        <Button x:Name="btCancel" Content="Отмена" HorizontalAlignment="Right" Margin="0,0,10,10" Width="75" Height="22" VerticalAlignment="Bottom" Click="btCancel_Click_1"/>
        <Label Content="Введите название новой коллекции" Height="34" VerticalAlignment="Top"/>
        <TextBox x:Name="tbCollectName" Margin="10,0,10,37" TextWrapping="Wrap" Text="Новая коллекция" Height="23" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
